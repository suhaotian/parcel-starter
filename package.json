{
  "name": "parcel-starter",
  "alias": {
    "/": "./"
  },
  "targets": {
    "client": {
      "source": "src/client/index.html",
      "distDir": "dist/client/",
      "publicUrl": "./",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    },
    "server": {
      "source": "src/server/index.ts",
      "distDir": "dist/server/",
      "engines": {
        "node": "^12.0.0"
      }
    },
    "server-dev": {
      "source": "src/server/index.ts",
      "distDir": "dist/server-dev/",
      "engines": {
        "node": "> 12.0.0"
      }
    }
  },
  "browserslist": [
    "> 0.5%, last 2 versions, not dead"
  ],
  "scripts": {
    "start": "yarn build:server-dev && concurrently \"yarn nodemon:server\" \"yarn start:server\" \"yarn start:client\"",
    "start:client": "parcel serve --target client --open chrome -p 3020",
    "start:server": "parcel watch --target server-dev",
    "nodemon:server": "nodemon dist/server-dev/index.js",
    "build": "concurrently \"yarn build:server\" \"yarn build:client\"",
    "build:server": "parcel build --target server",
    "build:server-dev": "parcel build --target server-dev",
    "build:client": "parcel build --target client",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "fastify": "^3.27.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.29",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.15",
    "parcel": "^2.3.0",
    "process": "^0.11.10",
    "typescript": "^4.4.4"
  },
  "version": "1.0.0",
  "author": "suhaotian",
  "license": "MIT"
}
